      "pattern": "^1[3-9]\\d{9}$",
      "description": "手机号码"
    },
    "出生日期": {
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
      "description": "出生日期，格式YYYY-MM-DD"
    },
    "数量": {
      "pattern": "^[0-9]+(\\.[0-9]+)?$",
      "description": "数量"
    },
    "单价": {
      "pattern": "^[0-9]+(\\.[0-9]+)?$",
      "description": "单价"
    }
  },
  "relations": [
    {
      "fields": ["城市", "邮编"],
      "condition": "城市=邮编"
    }
  ]
